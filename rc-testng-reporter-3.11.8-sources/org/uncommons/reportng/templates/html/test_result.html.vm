<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="$messages.locale.language" lang="$messages.locale.language">
<head>
  <title>$meta.reportTitle - $result.getName() - $status</title>
  <meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
  <meta name="description" content="TestNG unit test results." />
  <script  type="text/javascript" src="jquery-1.4.3.min.js"></script>
  <link href="reportng.css" rel="stylesheet" type="text/css" />
  #if ($meta.stylesheetPath)
  <link href="custom.css" rel="stylesheet" type="text/css" />
  #end
  <link rel="stylesheet" href="jquery.fancybox-1.3.4.css" type="text/css" media="screen" />
  <script type="text/javascript" src="jquery.fancybox-1.3.4.pack.js"></script>
  <script type="text/javascript" src="reportng.js"></script>


</head>
    <body onload="checkCookie()">
<h1>$result.getName()</h1>
<p>
    #if($duration)
    #set ($durationInfo = $utils.formatDuration($duration))
    #else
    #set ($durationInfo = 'undefined')
    #end
  $messages.getString("testDuration"): ${durationInfo}s

</p>
<p>
  $messages.getString("testStart"): ${start}  | Time stamp: <img src="timeStampOff.png" onclick="toggleTimeStamp()" title="Time stamp switch" id="timeStampSwitch" alt="switch"></img>
</p>

#set ($testResultName = $result.name)
#set ($id = 0)
<table class="resultsTable">
  <tr class="header big">
    <td colspan="3" style="padding-top:5px">
        <div style="float:left">
            <span>$testResultName</span>
            <a href="logs/$utils.getAttribute($result, 'logFile')" title="Full test log" target="_blank"><img src="log_icon.png"/></a>
            <span>&nbsp;</span><span>&nbsp;</span>
            #if($utils.dashboardEnabled($result))<a href="$utils.getDashLink()testTempid=$utils.getAttribute($result,"test_unique_id")" target="_blank"><img src="ci-dashboard.png" title="See test results in dashboard"></a>#end
        </div>
        <div style="float:right; font-size: 10px; font-weight: normal">
            <div style="font-weight: bold" onclick="toggleElement('goto', 'block'); toggle_my('goto')">
                Go to <span id="toggle-goto" class="toggle">&#x25bc;</span>
            </div>
            <div style="display: none" id="goto">
                #if ($utils.getAttribute($result, "overviewFileName") != "")
                    <a href='$utils.getAttribute($result, "overviewFileName")'>$messages.getString("bySuites") view</a>
                    <br/>
                #end
                #if ($utils.getAttribute($result,"groupOverviewFileName") != "")
                    <a href='$utils.getAttribute($result,"groupOverviewFileName")'>$messages.getString("byGroups") view</a>
                    <br/>
                #end
                #if ($result.getAttribute("parents").size() > 0)
                    #foreach ($link in $result.getAttribute("parents"))
                        <a href='$link.get(0)'>$link.get(1)</a>
                        <br/>
                    #end
                #end
            </div>
        </div>
    </td>
  </tr>
  #set ($testResult = $result)
  #parse("org/uncommons/reportng/templates/html/test-results.html.vm")
</table>

</body>
</html>
